<%- model_class = Instructor -%>
<div class="page-header">
 <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dt><strong><%= image_tag @instructor.avatar.url(:small) %></strong></dt>

<div class='instructor'>
  <p><%= @instructor.id %></p>
  <p><%= @instructor.nombres %></p>
  <p><%= @instructor.apellidos %></p>
  <p><%= @instructor.cedula %></p>
  <p><%= @instructor.email %></p>
  <p><%= @instructor.direccion %></p>
  <p><%= @instructor.telefono %></p>
  <p><%= @instructor.fch_nacimiento %></p>
  <p><%= @instructor.materia_id %></p>
</div>